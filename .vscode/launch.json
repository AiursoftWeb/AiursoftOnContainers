{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Account",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildAccount",
            "program": "${workspaceFolder}/Account/bin/Debug/netcoreapp2.0/Aiursoft.Account.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Account",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
