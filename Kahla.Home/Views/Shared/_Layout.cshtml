<!doctype html>
<html lang="en" class="windows">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="content-language" content="en-gb">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="@ServiceLocation.CDN/dist/AiurCore.min.css" rel="stylesheet">
    <link href="@ServiceLocation.CDN/dist/AiurProduct.min.css" rel="stylesheet">
    <title>Kahla App | @Values.ProjectName</title>
</head>
<body>
    @RenderBody()
    <!-- <footer class="py-6 px-3 text-gray text-center">
        <p class="copyright">© @DateTime.Now.Year @Values.ProjectName, Inc. All rights reserved.</p>
    </footer> -->
    <div class="footer container-lg px-3" role="contentinfo">
        <div class="position-relative d-flex flex-justify-between pt-6 pb-2 mt-6 f6">
            <ul class="list-style-none d-flex flex-wrap ">
                @foreach(var foot in Values.Footer)
                {
                    <li class="mr-3"><a href="@foot.Value">@foot.Key</a></li>
                }
            </ul>
        </div>
        <div class="d-flex flex-justify-center pb-6">
            <span class="f6 text-gray-light"></span>
        </div>
    </div>
    <script src="@ServiceLocation.CDN/dist/AiurCore.min.js"></script>
    <script src="@ServiceLocation.CDN/dist/AiurProduct.min.js"></script>
</body>
</html>